{%- capture cl -%}
    {%- if size == 'extra-small' -%}
        w-[12px] h-[12px]
    {%- elsif size == 'small' -%}
        w-[16px] h-[16px]
    {%- elsif size == 'medium-small' -%}
        w-[18px] h-[18px]
    {%- elsif size == 'medium' -%}
        w-[20px] h-[20px]
    {%- elsif size == 'custom' -%}
        {{ class | strip }}
    {%- else -%}
        w-[24px] h-[24px]
    {%- endif -%}
{%- endcapture -%}
{% assign class = cl | strip_newlines | strip %}

